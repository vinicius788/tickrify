# Clerk Authentication
# Get your publishable key from https://dashboard.clerk.com/
VITE_CLERK_PUBLISHABLE_KEY=YOUR_CLERK_PUBLISHABLE_KEY_HERE

# Backend API
# Defina SEMPRE a URL do backend em produção (ex: https://api.tickrify.com)
# Em desenvolvimento, mantenha localhost:
VITE_API_URL=http://localhost:3001

# Stripe (opcional - para pagamentos)
VITE_STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY_HERE
# Ative apenas se STRIPE_PRICE_PRO_ANNUAL estiver configurado no backend
VITE_ENABLE_ANNUAL_BILLING=false
